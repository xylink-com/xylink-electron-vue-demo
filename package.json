{
  "name": "xy-electron-vue-demo",
  "version": "3.10.0",
  "private": true,
  "description": "electron sdk vue",
  "license": "MIT",
  "author": "XYLink Inc",
  "scripts": {
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "dev": "cross-env NODE_ENV=development vue-cli-service electron:serve",
    "build:32": "cross-env NODE_ENV=production vue-cli-service electron:build --win --ia32",
    "build:mac": "cross-env NODE_ENV=production vue-cli-service electron:build --mac"
  },
  "main": "background.js",
  "dependencies": {
    "@electron/remote": "^2.0.9",
    "@element-plus/icons-vue": "^2.0.10",
    "@xylink/xy-electron-sdk": "^3.10.1",
    "clone-deep": "^4.0.1",
    "core-js": "^3.8.3",
    "dayjs": "^1.11.7",
    "electron": "24.8.3",
    "element-plus": "^2.2.28",
    "js-base64": "^3.7.2",
    "pinia": "^2.0.21",
    "vue": "^3.2.13",
    "vue-router": "^4.1.6",
    "vue3-clipboard": "^1.0.0",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "cross-env": "^7.0.2",
    "electron-devtools-installer": "^3.1.0",
    "electron-store": "^8.0.2",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "native-ext-loader": "^2.3.0",
    "node-loader": "^1.0.1",
    "sass": "^1.58.3",
    "sass-loader": "^10.1.1",
    "vue-cli-plugin-electron-builder": "~2.1.1"
  },
  "overrides": {
    "vue-cli-plugin-electron-builder/electron-builder": "^23.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/multi-word-component-names": [
        "warn",
        {
          "ignores": [
            "index",
            "Video",
            "External",
            "device"
          ]
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
